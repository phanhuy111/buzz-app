{
  "name": "buzz",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "android": "react-native run-android --variant=devDebug",
    "android:pro": "react-native run-android --variant=productionDebug",
    "ios": "fastlane ios set_dev_info && react-native run-ios --scheme=\"buzz-dev\"",
    "ios:pro": "fastlane ios set_production_info && react-native run-ios --scheme=\"buzz\"",
    "CodePush:ios": "appcenter CodePush release-react -a buzz-app/buzz-ios -d Production",
    "CodePushDev:ios": "appcenter CodePush release-react -a buzz-app/buzz-ios -d Development",
    "CodePush:android": "appcenter codepush release-react -a buzz-app/buzz-android -d Production",
    "CodePushDev:android": "appcenter codepush release-react -a buzz-app/buzz-android -d Development",
    "lint": "eslint .",
    "start": "react-native start",
    "reformat": "prettier --write \"./**/*.{js,jsx,ts,tsx,json}\"",
    "test": "jest",
    "build:android:dev": "cd android && ./gradlew clean && ./gradlew assembleDevRelease && cd ..",
    "build:android:dev:bundle": "cd android && ./gradlew clean && ./gradlew bundleDevRelease && cd ..",
    "build:android:production": "cd android && ./gradlew clean && ./gradlew assembleProductionRelease && cd ..",
    "build:android:production:bundle": "cd android && ./gradlew clean && ./gradlew bundleProductionRelease && cd ..",
    "build:ios:dev": "xcodebuild archive -allowProvisioningUpdates -quiet -workspace ./ios/buzz.xcworkspace -scheme buzz-dev -configuration Distribution -archivePath ./ios/build/buzz.xcarchive -destination 'generic/platform=iOS'",
    "build:ios:production": "xcodebuild archive -allowProvisioningUpdates -quiet -workspace ./ios/buzz.xcworkspace -scheme buzz -configuration Distribution -archivePath ./ios/build/buzz.xcarchive -destination 'generic/platform=iOS'",
    "export:ios": "xcodebuild -allowProvisioningUpdates -exportArchive -archivePath \"./ios/build/buzz.xcarchive\" -exportPath \"./ios/build\" -exportOptionsPlist \"./ios/buzz/ExportOptions.plist\" -destination 'generic/platform=iOS'",
    "unlock-ci": "security unlock-keychain -p \"blackwing\"",
    "extract:locales": "formatjs extract \"src/**/*.{js,jsx,ts,tsx}\" --ignore \"src/**/*.d.ts\" --out-file ./src/lang/temp.json --flatten && formatjs compile ./src/lang/temp.json --out-file ./src/lang/vi.json && rm ./src/lang/temp.json"
  },
  "dependencies": {
    "@gorhom/bottom-sheet": "^4.4.6",
    "@hookform/resolvers": "^3.1.0",
    "@invertase/react-native-apple-authentication": "^2.2.2",
    "@notifee/react-native": "^7.8.0",
    "@pusher/pusher-websocket-react-native": "^1.2.2",
    "@react-native-async-storage/async-storage": "^1.18.1",
    "@react-native-firebase/analytics": "^18.1.0",
    "@react-native-firebase/app": "^17.4.2",
    "@react-native-firebase/auth": "^17.4.2",
    "@react-native-firebase/messaging": "^17.4.2",
    "@react-native-google-signin/google-signin": "^9.0.2",
    "@react-navigation/bottom-tabs": "^6.5.7",
    "@react-navigation/elements": "^1.3.17",
    "@react-navigation/native": "^6.1.6",
    "@react-navigation/native-stack": "^6.9.12",
    "@react-navigation/stack": "^6.3.16",
    "@reduxjs/toolkit": "^1.9.3",
    "@sentry/react-native": "^5.8.1",
    "@shopify/flash-list": "^1.4.3",
    "@tanstack/react-query": "^4.29.3",
    "axios": "^1.3.4",
    "eslint-config-airbnb": "19.0.4",
    "lodash": "^4.17.21",
    "react": "18.2.0",
    "react-content-loader": "^6.2.1",
    "react-hook-form": "^7.43.9",
    "react-intl": "^6.4.2",
    "react-native": "0.71.5",
    "react-native-actionsheet": "^2.4.2",
    "react-native-blob-util": "^0.18.2",
    "react-native-code-push": "^8.0.2",
    "react-native-config": "^1.5.0",
    "react-native-device-info": "^10.6.0",
    "react-native-fast-image": "^8.6.3",
    "react-native-gesture-handler": "^2.9.0",
    "react-native-keyboard-controller": "^1.6.0",
    "react-native-keychain": "^8.1.1",
    "react-native-reanimated": "^3.2.0",
    "react-native-safe-area-context": "^4.5.0",
    "react-native-screens": "^3.20.0",
    "react-native-sticky-parallax-header": "^1.0.0-rc.9",
    "react-native-svg": "^13.9.0",
    "react-native-vector-icons": "^10.0.0",
    "react-redux": "^8.0.5",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.2.3",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@formatjs/cli": "^6.1.1",
    "@react-native-community/eslint-config": "^3.2.0",
    "@tsconfig/react-native": "^2.0.2",
    "@types/jest": "^29.2.1",
    "@types/lodash": "^4.14.194",
    "@types/react": "^18.0.24",
    "@types/react-native": "^0.70.6",
    "@types/react-test-renderer": "^18.0.0",
    "@types/redux-logger": "^3.0.9",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.37.0",
    "babel-jest": "^29.2.1",
    "babel-plugin-formatjs": "^10.5.1",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "eslint": "8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.4",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsdoc": "^39.2.9",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.73.9",
    "patch-package": "^6.5.0",
    "prettier": "^2.4.1",
    "react-native-svg-transformer": "^1.0.0",
    "react-test-renderer": "18.2.0",
    "reactotron-react-native": "^5.0.3",
    "reactotron-react-query": "^1.0.2",
    "reactotron-redux": "^3.1.3",
    "reactotron-redux-saga": "^4.2.3",
    "typescript": "4.8.4"
  },
  "jest": {
    "preset": "react-native"
  }
}